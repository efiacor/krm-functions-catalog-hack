apiVersion: kpt.dev/v1
kind: Kptfile
metadata:
  name: example
  annotations:
    config.kubernetes.io/local-config: "true"
pipeline:
  mutators:
    - image: ghcr.io/kptdev/krm-functions-catalog/set-annotations:unstable
      configPath: fn-config.yaml
      selectors:
        - kind: ConfigMap
        - kind: MyResource
