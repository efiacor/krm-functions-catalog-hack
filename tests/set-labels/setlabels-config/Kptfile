apiVersion: kpt.dev/v1
kind: Kptfile
metadata:
  name: example
  labels:
    color: orange
    fruit: apple
pipeline:
  mutators:
    - image: ghcr.io/kptdev/krm-functions-catalog/set-labels:unstable
      configPath: fn-config.yaml
      selectors:
        - kind: ConfigMap
        - kind: Service
